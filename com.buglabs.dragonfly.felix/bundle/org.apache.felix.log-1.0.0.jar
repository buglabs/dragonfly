<script type="text/javascript">
	$("li.children").click(function () {
		var menuItem = $(this).attr("id");
		var subMenuId = menuItem.replace("menuItem","");
		var display = $("ul#subMenu"+subMenuId).css("display");
		var duration = 300;
		if(display=="block") {
			$("ul#subMenu"+subMenuId).slideUp(duration);
			$("li#menuItem"+subMenuId).removeClass('active');
		} else {
			$("ul#subMenu"+subMenuId).slideDown(duration);
			$("li#menuItem"+subMenuId).addClass('active');
		}
		return false;
	});
</script>

